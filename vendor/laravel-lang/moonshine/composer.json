{
    "name": "laravel-lang/moonshine",
    "description": "Translation for MoonShine admin panel",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "languages",
        "lang",
        "translations",
        "moonshine"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@dragon-code.pro"
        },
        {
            "name": "Laravel Lang Team",
            "homepage": "https://laravel-lang.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "laravel-lang/publisher": "^15.0 || ^16.4.1"
    },
    "require-dev": {
        "dragon-code/support": "^6.15.2",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0",
        "laravel-lang/lang": "^14.8 || ^15.14",
        "laravel-lang/status-generator": "^2.12",
        "phpunit/phpunit": "^10.0 || ^11.0 || ^12.0",
        "symfony/var-dumper": "^6.3 || ^7.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "LaravelLang\\MoonShine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dragon-code/codestyler": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLang\\MoonShine\\ServiceProvider"
            ]
        }
    }
}
